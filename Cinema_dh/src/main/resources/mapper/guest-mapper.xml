<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.cinema.mapper.GuestMapper">
	<insert id="insertGuest" parameterType="guest">
		insert into guest values(#{gEmail}, #{gName})
	</insert>
	
	<select id="guestLogin" parameterType="guest" resultType="guest">
		select * from guest where g_email = #{gEmail}
	</select>
</mapper>