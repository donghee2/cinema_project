<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.cinema.mapper.NonMemberMapper">
	<insert id="insertNonMember" parameterType="nonmember">
		insert into nonmember values(#{nEmail}, #{nName})
	</insert>
	
	<select id="nonMemberLogin" parameterType="nonmember" resultType="nonmember">
		select * from nonmember where n_email = #{nEmail}
	</select>
</mapper>